<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="itp341.tang.daiyi.finalprojecttangdaiyi">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <uses-permission android:name="android.permission.INTERNET"/>
        <uses-permission android:name="android.permission.RECORD_AUDIO" />
        <meta-data android:name="DATABASE" android:value="sugar_finalprojecttangdaiyi.db" />
        <meta-data android:name="VERSION" android:value="6" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="itp341.tang.daiyi.finalprojecttangdaiyi.model" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PhotoAlbum"
            android:label="PhotoAlbum" >
        </activity>

        <activity
            android:name=".StartRecording"
            android:label="startRecording" >
        </activity>

        <activity
            android:name=".AddPhoto"
            android:label="addPhoto" >
        </activity>

        <activity
            android:name=".TodayActivity"
            android:label="today" >
        </activity>

        <activity
            android:name=".activities.MyCalendarActivity"
            android:label="calender" >
        </activity>

        <activity
            android:name=".CheckPhoto"
            android:label="check" >
        </activity>

        <activity
            android:name=".Recording"
            android:label="recording" >
        </activity>

        <activity
            android:name=".FinishRecording"
            android:label="finishRecording" >
        </activity>

        <activity
            android:name=".Delete"
            android:label="delete" >
        </activity>

    </application>

</manifest>